{
  "mcpServers": {
    "mygemini-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://localhost:8000/mcp",
        "--header",
        "X-API-Key: ${MCP_CLIENT_KEY}"
      ],
      "env": {
        "MCP_CLIENT_KEY": "dev-key-1"
      },
      "disabled": false
    }
  }
}
